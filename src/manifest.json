{
  "name": "Timetable app clone",
  "description": "A Vue.js web extension",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "permissions" : ["notifications"],
  "browser_action": {
    "default_title": "timetable-extension",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],"persistent": false
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}
